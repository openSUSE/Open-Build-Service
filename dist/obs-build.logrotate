/srv/www/obs/webui/log/apache*log {
  compress
  dateext
  maxage 365
  rotate 30
  size +4096k
  notifempty
  missingok
  copytruncate
  create 664 wwwrun www
}

/srv/www/obs/webui/log/production.log {
  compress
  dateext
  maxage 365
  rotate 30
  size +4096k
  notifempty
  missingok
  copytruncate
  create 664 wwwrun www
  sharedscripts
}

