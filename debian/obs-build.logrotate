/srv/www/obs/webui/log/access.log {
  compress
  dateext
  maxage 365
  rotate 30
  size +4096k
  notifempty
  missingok
  copytruncate
  create 664 www-data www-data
}

/srv/www/obs/webui/log/production.log {
  compress
  dateext
  maxage 365
  rotate 30
  size +4096k
  notifempty
  missingok
  copytruncate
  create 664 www-data www-data
  sharedscripts
}

