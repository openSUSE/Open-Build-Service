#!/bin/sh -e

. /usr/share/debconf/confmodule

case "$1" in
	remove)
		db_get obs-api || true
		if [ "$RET" = "true" ]; then
			. /usr/share/dbconfig-common/dpkg/prerm.mysql
			dbc_go obs-api $@
		fi
		invoke-rc.d obsapidelayed stop || true
	;;

	purge|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
	;;
esac

