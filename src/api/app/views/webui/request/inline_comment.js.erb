$('#flash').empty();
$('#comment<%= @line %>').html("<%= escape_javascript(render(partial: 'webui/request/inline_comment', locals: { comment: @comment, commentable: @active_action, diff_ref: @line, file_name: @file_name })) %>");
$("#comment<%= @line %> textarea").focus();
$("#comment<%= @line %>").on('click', '.cancel-comment', function (e) {
  $('#comment<%= @line %>').html("");
});
